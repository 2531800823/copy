{
  "name": "copy",
  "version": "1.0.0",
  "packageManager": "pnpm@10.10.0",
  "description": "",
  "author": "2531800823@qq.com",
  "license": "ISC",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm sort-deps",
    "sync": "syncpack fix-mismatches",
    "sync:check": "syncpack list-mismatches",
    "sync:lock": "syncpack set-semver-ranges",
    "sort-deps": "node ./scripts/shared/sortDependencies.mjs",
    "lint": "eslint . --fix",
    "prettier": "node ./scripts/prettier/index.mjs write-changed",
    "prettier-all": "node ./scripts/prettier/index.mjs write",
    "web": "pnpm --filter web",
    "ds": "pnpm --filter desktop",
    "ser": "pnpm -F server",
    "dev:web": "pnpm --filter web dev",
    "dev:desktop": "pnpm --filter desktop dev",
    "dev:server": "pnpm --filter server dev",
    "start": "npm-run-all --parallel dev:web dev:desktop ",
    "build:web": "pnpm --filter web build",
    "build:desktop": "pnpm --filter desktop build:win",
    "build": "pnpm build:web && pnpm build:desktop"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@eslint-react/eslint-plugin": "1.48.5",
    "@eslint/js": "9.27.0",
    "@types/node": "22.15.26",
    "chalk": "5.4.1",
    "eslint": "9.27.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "11.0.2",
    "globals": "16.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "syncpack": "13.0.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.0",
    "unplugin-auto-import": "^20.0.0"
  }
}
